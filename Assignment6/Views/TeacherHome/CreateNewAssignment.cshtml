@{
    ViewBag.Title = "CreateNewAssignment";
    Layout = "~/Views/Shared/View1.cshtml";
}



<div class="f">
    <h2 align="center">Create New Assignment</h2>

<form action="/TeacherHome/saveNewAssignment" , method="post" , enctype="multipart/form-data">

Assignment Title <input type="text" , id="Title" , name="Title" ,class="search">
<br/>
Assignment Display Name <input type="text" , id="AssName" name="DisplayName" , class="search">
<br/>
Difficulty Level <input type="text" , id="AssLevel" , name="DifficultyLevel" , class="search">
<br/>
Total Marks <input type="number" , id="AssMarks" , name="Marks" , class="search">
<br/>
Format <input type="text" , id="AssFormat" , name="Format" , class="search">
<br/>
Deadline <input type="datetime" , id="AssDeadline" , name="Deadline" , class="search">
<br/>
Upload File <input type="file" , id="AssFile" , name="ProblemLink" , class="search">
<input type="submit" , value="Create" , class="btn">
    </form>
    </div>